<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="configurator_settings_view_form" model="ir.ui.view">
        <field name="name">configurator.settings.view.form</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="99" />
        <field name="inherit_id" ref="base.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//form" position="inside">
                <app data-string="Product Configurator" string="Product Configurator" name="product_configurator" />
            </xpath>
        </field>
    </record>

</odoo>
